#include <stdio.h>
#include <stdlib.h>
#include <assert.h>

int gcd(int a, int b);
int ost(int a, int b);

int main(){
    int a, b, ans, res;
    res = scanf("%d%d", &a, &b);
    if (res != 2 || b == 0){
        printf("Error");
        abort();
    }
    ans = gcd(a, b);
    printf("%d", ans);
    return 0;
}

int gcd(int a, int b){
    int r = ost(a, b);
    while (r != 0){
        a = b;
        b = r;
        r = ost(a, b);
    }

    return b;
}

int ost(int a, int b){
    int r;
    assert(b != 0);
    r = a % b;
    if (r < 0){
        r += abs(b);
    }
    return r;
}
