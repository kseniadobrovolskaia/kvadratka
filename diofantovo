#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include "assert.h"


int Comparison(int* a, int* b);
int NOD(int a, int b);
int ost(int a, int b);
int Solution(int a, int b, int c, int* ans1, int* ans2);


int main() {
    int a, b, c, a0, b0, compare, nod, ans1, ans2;
    assert(scanf("%d%d%d", &a, &b, &c) == 3);


    compare = Comparison(&a, &b);

    a0 = a;
    b0 = b;

    nod = NOD(a0, b0);

    a = a0 / nod;
    b = b0 / nod;
    if (c % nod != 0){
        printf("NONE\n");
        return 0;
    }
    c = c / nod;

    Solution(a, b, c, &ans1, &ans2);

    if (compare){
        int k;
        k = ans1;
        ans1 = ans2;
        ans2 = k;
    }
    printf("%d %d\n", ans1 * c, ans2 * c);
    return 0;
}


int Comparison(int* a, int* b) {
    int c;
    if (*a > *b){
        return 0;
    }
    c = *a;
    *a = *b;
    *b = c;
    return 1;
}

int NOD(int a, int b) {
    int c;
    c = ost(a, b);
    while (c != 0) {
        a = b;
        b = c;
        c = ost(a, b);
    }
    return abs(b);
}

int Solution(int a, int b, int c, int* ans1, int* ans2) {
    int p, q, counta2, countb2, counta1, countb1, counta, countb;
    q = a % b;
    counta1 = 0;
    countb1 = 1;
    counta = 1;
    countb = 0;

    if (q == 0){
        if (a == c){
            *ans1 = 1;
            *ans2 = 0;
            return 0;
        }
        *ans1  = 0;
        *ans2 = 1;
    }

    if (q == 1) {
        counta1 = 1;
        countb1 = -a / b;
    } else {
        while (q != 1) {
            if (q == 0) {
                break;
            }
            p = a / b;
            if (a % b < 0){
                if (b < 0){
                    p += 1;
                } else{
                    p -= 1;
                }
            }
            q = ost(a, b);
            counta2 = counta - counta1 * p;
            countb2 = countb - countb1 * p;
            a = b;
            b = q;
            counta = counta1;
            countb = countb1;
            counta1 = counta2;
            countb1 = countb2;
        }
    }
    *ans1 = counta1;
    *ans2 = countb1;

    return 0;
}

int ost(int a, int b){
    int r;
    assert(b != 0);
    r = a % b;
    if (r < 0){
        r += abs(b);
    }
    return r;
}
